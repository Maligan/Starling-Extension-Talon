<?xml version="1.0" encoding="utf-8"?>
<application xmlns="http://ns.adobe.com/air/application/20.0">

  <id>TalonBrowser</id>
  <filename>Talon Browser</filename>
  <name>Talon Browser</name>
  <versionNumber>0.2.1</versionNumber>

  <initialWindow>
    <content>%CONTENT%</content>
    <visible>false</visible>
    <renderMode>direct</renderMode>
    <depthAndStencil>true</depthAndStencil>
  </initialWindow>

  <icon>
    <image16x16>icons/icon16.png</image16x16>
    <image32x32>icons/icon32.png</image32x32>
    <image48x48>icons/icon48.png</image48x48>
    <image128x128>icons/icon128.png</image128x128>
  </icon>

</application>
